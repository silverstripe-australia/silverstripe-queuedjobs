{
    "name": "silverstripe/queuedjobs",
    "description": "A framework for defining and running background jobs in a queued manner",
    "type": "silverstripe-module",
    "keywords": ["silverstripe", "jobs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@silverstripe.com.au"
        },
        {
            "name": "Damian Mooyman",
            "email": "damian@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4@dev",
        "silverstripe/admin": "^1.0@dev",
        "asyncphp/doorman": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\QueuedJobs\\": "src/"
        }
    }
}
